#include <iostream>
using namespace std;

int a[10000][10000];

void initialise(int n)
{
    for(int i=1;i<=n;i++)
     {
         for(int j=1;j<=n;j++)
          a[i][j]=0;
     }
}

int main()
{
   int nodes,edges; cin>>nodes>>edges;
   initialise(nodes);
   while(edges--)
   {
       int x,y;
       cin>>x>>y;
       a[x][y]=1;
   }
   for(int i=1;i<=nodes;i++)
     {
         for(int j=1;j<=nodes;j++)
         {
           cout<<a[i][j]<<" ";
         }
         cout<<endl;
     }

    return 0;
}
